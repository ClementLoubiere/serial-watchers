{% extends 'base.html.twig' %}{% block body %}    {#-------------------------------------------------------------------------------#}    {#------------------------------ INTERFACE UTILISATEUR---------------------------#}    {#-------------------------------------------------------------------------------#}    {#content wrapper #}    <div id="content-wrapper">    {#<div class="container-fluid">#}    <!-- Icon Cards-->    <div class="row">        {# affichages card toutes les séries #}        <div class="col-xl-3 col-sm-6 mb-3">            <div class="card text-white o-hidden h-100 bg-blue">                <div class="card-body">                    <div class="card-body-icon">                    </div>                    <div class="mr-5">Toutes les séries</div>                </div>                <a class="card-footer text-white clearfix small z-1" href="{{ path('app_series_api') }}">                    <span class="float-left">Plus d'infos</span>                    <span class="float-right">                    <i class="fas fa-angle-right"></i>                  </span>                </a>            </div>        </div>        {# affichages card favoris #}        <div class="col-xl-3 col-sm-6 mb-3">            <div class="card text-white bg-yellow o-hidden h-100">                <div class="card-body">                    <div class="card-body-icon">                    </div>                    <div class="mr-5">Favoris</div>                </div>                {#Lien vers l'index épisodes#}                <a class="card-footer text-white clearfix small z-1" href="{{ path('app_series_afficherfav') }}">                    <span class="float-left">Plus d'infos</span>                    <span class="float-right">                    <i class="fas fa-angle-right"></i>                  </span>                </a>            </div>        </div>        {# affichages card Nouveautés #}        <div class="col-xl-3 col-sm-6 mb-3">            <div class="card text-white bg-info o-hidden h-100">                <div class="card-body">                    <div class="card-body-icon">                    </div>                    <div class="mr-5">Nouveautés</div>                </div>                <a class="card-footer text-white clearfix small z-1" href="{{ path('app_user_newserie') }}">                    <span class="float-left">Plus d'infos</span>                    <span class="float-right">                    <i class="fas fa-angle-right"></i>                  </span>                </a>            </div>        </div>        {# affichages card Prochainnement #}        <div class="col-xl-3 col-sm-6 mb-3">            <div class="card text-white bg-purple o-hidden h-100">                <div class="card-body">                    <div class="card-body-icon">                        <i class="fas fa-fw fa-life-ring"></i>                    </div>                    <div class="mr-5"></div>                </div>                <a class="card-footer text-white clearfix small z-1" href="{{ path('app_user_nextseries') }}">                    <span class="float-left">Prochainement</span>                    <span class="float-right">                    <i class="fas fa-angle-right"></i>                  </span>                </a>            </div>        </div>    </div>    <!-- DERNIERS EPISODES -->    <div class="card mb-3">        <div class="card-header">            <i class="fas fa-chart-area"></i>            Derniers épisodes        </div>        <div class="card-body">            <div class="row row-striped">                <div class="col-2 text-right">                    {# affichage jour #}                    <h1 class="display-4"><span class="badge badge-secondary">23</span></h1>                    {# Affiche Mois #}                    <h2>OCT</h2>                </div>                <div class="col-10">                    {#Nom de la série #}                    <h3 class="text-uppercase"><strong>NameSérie</strong></h3>                    <ul class="list-inline">                        {#jour de diffusion#}                        <li class="list-inline-item"><i class="fa fa-calendar-o" aria-hidden="true"></i> Monday</li>                        {#heure de diffusion#}                        <li class="list-inline-item"><i class="fa fa-clock-o" aria-hidden="true"></i> 12:30 PM -                            2:00 PM                        </li>                        {#numéro de l'épisode diffusé#}                        <li class="list-inline-item"><i class="fa fa-location-arrow" aria-hidden="true"></i> Cafe                        </li>                    </ul>                </div>            </div>            <canvas id="myAreaChart" width="100%" height="30"></canvas>        </div>        {#lien vers la fiche série#}        <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div>        {#</div>#}    </div>    <!-- /.container-fluid -->    </div>{% endblock %}